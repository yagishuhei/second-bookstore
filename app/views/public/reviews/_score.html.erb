<!--レビュー５段階評価-->
<script>
  $('#score_<%= index %>').empty();
  $('#score_<%= index %>').raty({
    //imageを表示
    starOff: '<%= asset_path('star-off.png') %>',
    starOn: '<%= asset_path('star-on.png') %>',
    starHalf: '<%= asset_path('star-half.png') %>',
    //半値も表示できる
    half: true,
    //固定して表示できる
    readOnly: true,
    //呼び出したいカラムデータ
    score: <%= review.score %>,
  });
</script>