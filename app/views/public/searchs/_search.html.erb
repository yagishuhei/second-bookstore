<% if end_user_signed_in? %>
  <div class="search_form">
    <%= form_with url: search_path, local: true, method: :get do |f| %>
      <%= f.text_field :word %>
      <%= f.serect :renge, options_from_collection_for_select([['yu '],['rr']]) %>
      <%= f.serect :renge, options_from_collection_for_select([['一部検索']]) %>
      <%= f.submit "検索", class: "btn btn-primary" %>
    <% end %>
  </div>
<% end %>
